<FindBugsFilter>
    <Match>
        <Package name="com.vmware.dcm.generated"/>
    </Match>
    <Match>
        <Package name="com.vmware.dcm.generated.parser"/>
    </Match>
    <Match>
        <Package name="com.vmware.dcm.k8s.generated"/>
    </Match>
    <Match>
        <Package name="com.vmware.dcm.k8s.generated.tables"/>
    </Match>
    <Match>
        <Bug code="EI"/>
    </Match>
    <Match>
        <Bug code="EI2"/>
    </Match>
    <Match>
        <Bug pattern="DMI_RANDOM_USED_ONLY_ONCE"/>
    </Match>
    <Match>
        <!-- False positive when using text blocks -->
        <Bug pattern="VA_FORMAT_STRING_USES_NEWLINE"/>
    </Match>
    <Match>
        <!-- False positive in SchedulerTest -->
        <Bug pattern="NP_NONNULL_PARAM_VIOLATION"/>
    </Match>
</FindBugsFilter>
